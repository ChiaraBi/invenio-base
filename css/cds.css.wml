   background: transparent;
}
a.moreinfo:active {
   color: #060;
   background: transparent;
}
a.moreinfo:hover {
   color: #060;
   background: transparent;
}
a.img:hover {
   color: #00c;
   background: transparent;
}
a.img:active {
   color: #00c;
   background: transparent;
   font-weight: normal;
}
a.note:link {
